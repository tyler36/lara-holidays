{
    "name": "tyler36/lara-holidays",
    "description": "Help package to work with workdays, weekends & holidays",
    "license": "MIT",
    "homepage": "https://github.com/tyler36/lara-holidays",
    "keywords": [
        "Laravel",
        "LaraHolidays"
    ],
    "require": {
        "php": "^7.2",
        "illuminate/support": "~7|~8",
        "azuyalabs/yasumi": "^2.3"
    },
    "require-dev": {
        "phpunit/phpunit": "~8.0",
        "orchestra/testbench": "~5|~6",
        "phpstan/phpstan": "^1.4"
    },
    "autoload": {
        "psr-4": {
            "Tyler36\\LaraHolidays\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tyler36\\LaraHolidays\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Tyler36\\LaraHolidays\\LaraHolidaysServiceProvider"
            ],
            "aliases": {
                "LaraHolidays": "Tyler36\\LaraHolidays\\Facades\\LaraHolidays"
            }
        }
    }
}
